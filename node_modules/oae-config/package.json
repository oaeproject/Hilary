{
  "main": "./lib/api.js",
  "name": "oae-config",
  "oae": {
    "priority": 3
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/oaeproject/Hilary.git"
  },
  "version": "0.0.1",
  "xo": {
    "prettier": true,
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "ignores": [
      "Gruntfile.js"
    ],
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "prettier/prettier": "error",
      "no-use-before-define": "off"
    },
    "env": [
      "mocha"
    ],
    "overrides": [
      {
        "files": "tests/*.js",
        "rules": {
          "max-lines": 0,
          "no-unused-vars": "off"
        }
      }
    ]
  }
}
