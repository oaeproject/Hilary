  "scripts": {
    "test": "nyc grunt",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "lint": "xo --prettier --quiet *.js",
    "test-with-coverage": "nyc grunt && cat ./target/lcov.info | codacy-coverage"
  },
  "xo": {
    "prettier": true,
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "prettier/prettier": "error",
      "no-use-before-define": "off"
    },
    "env": [
      "mocha"
    ],
    "overrides": [
      {
        "files": "tests/*.js",
        "rules": {
          "max-lines": 0,
          "no-unused-vars": "off"
        }
      }
    ]
  },
  "snyk": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
